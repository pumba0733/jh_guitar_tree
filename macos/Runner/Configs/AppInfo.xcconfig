// AppInfo.xcconfig — 통합 Base Configuration (Debug/Release/Profile 공용)

// 1) CocoaPods: 빌드 구성별로 올바른 Pods-Runner.*.xcconfig 포함
#if CONFIGURATION == Debug
#include? "../../Pods/Target Support Files/Pods-Runner/Pods-Runner.debug.xcconfig"
#elif CONFIGURATION == Release
#include? "../../Pods/Target Support Files/Pods-Runner/Pods-Runner.release.xcconfig"
#else
#include? "../../Pods/Target Support Files/Pods-Runner/Pods-Runner.profile.xcconfig"
#endif

// 2) Flutter: 구성별 Flutter-<CONFIGURATION>.xcconfig 포함
#include "../../Flutter/Flutter-$(CONFIGURATION).xcconfig"

// 3) (선택) 프로젝트 공용 경고 규칙
#include "Warnings.xcconfig"

// 4) 👇 너의 기존 앱 메타(그대로 유지/필요 시 수정)
PRODUCT_NAME = guitartree
PRODUCT_BUNDLE_IDENTIFIER = com.example.guitartree
PRODUCT_COPYRIGHT = Copyright © 2025 com.example. All rights reserved.
